# Example environment configuration for ProfitScout

# --- Firebase client configuration ---
# API key from your Firebase project settings
NEXT_PUBLIC_FIREBASE_API_KEY=
# Auth domain, usually <project>.firebaseapp.com
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
# Unique Firebase project ID
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
# Default Cloud Storage bucket name
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
# Messaging sender ID used for FCM
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
# Unique app ID generated by Firebase
NEXT_PUBLIC_FIREBASE_APP_ID=
# Analytics measurement ID (optional)
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=

# --- Vertex AI Search (Gen App Builder) configuration ---
# Google Cloud project that hosts your data store
GCP_PROJECT_ID=
# Location of the data store, e.g., global or us-central1
GCP_LOCATION=
# ID of the Vertex AI Search data store
DATA_STORE_ID=
# Serving config ID, typically "default_search"
SERVING_CONFIG_ID=

# Path to service account credentials JSON (optional when using ADC)
GOOGLE_APPLICATION_CREDENTIALS=
